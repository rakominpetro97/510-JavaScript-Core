        // Task 1 
function calcRectangleArea(width, height){
    if (isNaN(width) || isNaN(height)) {
        throw "Parameter is not a number!";
    }
    return width * height;
}
try {
    alert(calcRectangleArea(9,6));
}
catch(e){
    console.log(e)
}
// Task 2
function checkAge(){
    if (isNaN(a > 18) || isNaN(a < 18)) {
        throw "Your not old enough!"
    }
}
try {
    prompt("How old you?");
    alert(checkAge(19));
}
catch(error){
console.log(error)
}
// Task 4
function showUsers(id){
    showUsers ([7, 12, -44, 22])
}
try {
    alert("Your ID isn't ok")
} catch (id) {
    alert("Your ID is ok")
    console.log(alert);
}

